Funcion d<-fresa(numerito,pregunta)
	Si j=numerito Entonces
		Escribir pregunta
		Leer a
	FinSi
FinFuncion


Algoritmo Coronavirus
	Escribir "Cuantos usuarios va a ingresar"
	Leer e
	
	Dimension test[e,2]
	
	Para i<-1 Hasta e Con Paso 1 Hacer
		Para j<-1 Hasta 2 Con paso 1 Hacer
			fresa(1,"Escribir nombre de usuario")
			fresa(2,"Tiene alguna comorbilidad")
			test[i,j]<-a;
		FinPara
    Fin Para
FinAlgoritmo